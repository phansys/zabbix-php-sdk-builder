{
    "name": "confirm-it-solutions/zabbix-php-sdk-builder",
    "type": "library",
    "description": "Builder for the PHP Zabbix SDK",
    "keywords": [
        "zabbix",
        "api"
    ],
    "homepage": "https://github.com/confirm/PhpZabbixApi",
    "license": "MIT",
    "authors": [
        {
            "name": "Dominique Barton",
            "email": "phpzabbixapi@confirm.ch",
            "homepage": "http://www.confirm.ch",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-pcre": "*",
        "ext-reflection": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpstan/phpstan": "^0.12.88",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZabbixApi\\": [
                "build/"
            ],
            "ZabbixApi\\Tests\\": [
                "tests/"
            ]
        }
    }
}
